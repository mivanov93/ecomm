{
    "name": "x0r/ecomm",
    "description": "Ecomm slim 2.0 php",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6.0",
        "slim/slim": "2.*",
        "doctrine/orm": "^2.5",
        "jeremykendall/slim-auth": "^1.0",
        "juliangut/slim-doctrine-middleware": "^0.3.2",
        "symfony/validator": "^3.3",
        "symfony/doctrine-bridge": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "": "src/",
            "Config\\": "config/config/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
